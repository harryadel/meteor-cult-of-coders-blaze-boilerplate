{
  "name": "meteor-cult-of-coders-blaze-boilerplate",
  "private": true,
  "scripts": {
    "start": "meteor npm i && meteor run",
    "dev": "meteor run --inspect",
    "test": "meteor test --driver-package meteortesting:mocha --settings settings.json",
    "lint": "eslint --cache --ext .js --ignore-path .eslintignore .",
    "lint:eslint:fix": "eslint --ext .js --ignore-path .eslintignore --fix .",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write '**/*.js' '**/*.jsx'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.10.3",
    "Package": "0.0.1",
    "bcrypt": "^5.0.0",
    "history": "^5.0.0",
    "meteor-node-stubs": "^1.0.0",
    "moment": "^2.27.0",
    "npm-check-updates": "^7.0.1",
    "simpl-schema": "^1.7.3"
  },
  "devDependencies": {
    "eslint": "^7.3.0",
    "eslint-config-meteor": "^0.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-meteor": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0"
  }
}
